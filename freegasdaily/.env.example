# Private key for deploying contracts (DO NOT SHARE THIS!)
# You can get this from your wallet (MetaMask, etc.)
PRIVATE_KEY=your_private_key_here

# Celoscan API Key for contract verification
# Get one at: https://celoscan.io/myapikey
CELOSCAN_API_KEY=your_celoscan_api_key_here

# Contract address (will be filled after deployment)
CONTRACT_ADDRESS=0x73Ce62F638A4De74B92307DfEC4837a4E6c6e3eE

# Network RPC URLs (optional, defaults are set in hardhat.config.js)
# CELO_RPC_URL=https://forno.celo.org
# ALFAJORES_RPC_URL=https://alfajores-forno.celo-testnet.org

# Gas reporting (optional)
# REPORT_GAS=true